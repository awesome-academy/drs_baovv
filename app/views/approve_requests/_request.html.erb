<tr>
  <td><%= i +1 %></td>
  <td><%= request.id %></td>
  <td><%= l(request.created_at, format: :short) %></td>
  <td><%= t "enum_status.#{request.status}" %></td>
  <td><%= link_to t(".show"), approve_request_path(request.id), {remote: true, "data-toggle": "modal", "data-target": "#show_dialog"} %></td>
</tr>
