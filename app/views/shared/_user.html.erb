<tr>
  <td><%= user_counter + 1 %></td>
  <td><%= user.id %></td>
  <td><%= user.division&.name %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.phone %></td>
  <td><%= l(user.birthday, format: :long) %></td>
  <td><%= user.skill %></td>
  <td>
    <% if user.division_id.blank? %>
      <%= link_to t(".add"), add_member_path(user), method: :patch, data: {confirm: t(".add_sure")} %>
    <% else %>
      <% unless user.manager?%>
        <%= link_to t(".delete"), manage_member_path(user), method: :patch, data: {confirm: t(".delete_sure")} %>
      <% end %>
    <% end %>
  </td>
</tr>
